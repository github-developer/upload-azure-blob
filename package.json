{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "format": "ts-standard --fix **/*.ts",
    "lint": "ts-standard src/*.ts",
    "package": "ncc build src/main.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@azure/identity": "^1.2.5",
    "@azure/storage-blob": "^12.5.0",
    "glob": "^7.1.6",
    "ts-standard": "^10.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@vercel/ncc": "^0.27.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
